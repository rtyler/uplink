  "devDependencies": {
    "@feathersjs/cli": "^3.8.2",
    "@sentry/types": "^4.0.1",
    "@types/feathersjs__configuration": "^1.0.1",
    "@types/feathersjs__express": "^1.1.4",
    "@types/feathersjs__feathers": "^3.0.4",
    "@types/jest": "^23.3.2",
    "@types/sequelize": "^4.27.25",
    "@types/winston": "^2.4.4",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize-cli": "^4.1.1",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.9",
