{
  "name": "uplink",
  "version": "1.0.0",
  "description": "Uplink receives bursts of Jenkins telemetry",
  "main": "src/index.js",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "start": "node ./src",
    "test": "jest"
