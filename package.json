    "@feathersjs/socketio": "^3.2.4",
    "@sentry/node": "^4.0.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "event-stream": "^3.3.4",
    "feathers-memory": "^2.2.0",
    "feathers-sequelize": "^3.1.2",
    "helmet": "^3.13.0",
    "jsonstream": "^1.0.3",
    "passport-github": "^1.1.0",
    "pg": "^7.4.3",
    "pg-query-stream": "^1.1.2",
    "pug": "^2.0.3",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.5.0",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
